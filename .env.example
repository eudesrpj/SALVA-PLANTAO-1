# Database (provisioned via Cloud SQL)
# Em Cloud Run, será injetado via Secret Manager
DATABASE_URL=postgresql://postgres:PASSWORD@HOST:5432/database_name?sslmode=require

# JWT Secrets (NUNCA commitar valores reais)
# Em Cloud Run, será injetado via Secret Manager
JWT_SECRET=your_jwt_secret_here
JWT_REFRESH_SECRET=your_jwt_refresh_secret_here

# Environment
NODE_ENV=production
